{
    "name": "pathways-frontend",
    "version": "1.16.0",
    "private": true,
    "devDependencies": {
        "@babel/core": "^7.7.7",
        "@lingui/babel-preset-react": "^2.9.0",
        "@lingui/cli": "^2.9.0",
        "@lingui/react": "^2.9.0",
        "@types/history": "^4.7.0",
        "@types/jest": "^24.0.25",
        "@types/ramda": "^0.26.39",
        "@types/react": "^16.9.0",
        "@types/react-native": "^0.60.15",
        "@types/react-redux": "^7.1.5",
        "@types/react-router-native": "^5.1.0",
        "@types/react-test-renderer": "^16.0.1",
        "@types/uuid": "^3.4.6",
        "ajv": "^6.5.3",
        "babel-core": "^7.0.0-bridge.0",
        "babel-plugin-macros": "^2.8.0",
        "babel-preset-expo": "^8.0.0",
        "handlebars": "^4.5.3",
        "jest-expo": "^36.0.0",
        "js-yaml": "^3.13.1",
        "react-dom": "16.9.0",
        "react-test-renderer": "^16.3.1",
        "ts-jest": "^24.2.0",
        "tslint": "^5.20.1",
        "tslint-immutable": "^6.0.1",
        "typescript": "^3.7.2"
    },
    "main": "./node_modules/expo/AppEntry.js",
    "scripts": {
        "postinstall": "bin/postinstall.sh",
        "lint": "tslint src/**/*.ts src/**/*.tsx",
        "build": "bin/buildFixtures.sh && tsc",
        "test": "node ./node_modules/jest/bin/jest.js",
        "cbt": "yarn clean && yarn build && yarn test",
        "bt": "yarn build && yarn test",
        "clean": "rimraf 'lib/**/*.js' 'lib/**/*.js.map' 'locale/_build/**/*.json'",
        "extract-strings": "lingui extract",
        "extract-strings-clean": "lingui extract --clean",
        "build-strings": "lingui compile --strict",
        "clean-strings": "rimraf 'locale/_build/**/*.json'",
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios"
    },
    "jest": {
        "preset": "jest-expo",
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testPathIgnorePatterns": [
            "/node_modules/",
            "/lib/",
            "/helpers/"
        ]
    },
    "dependencies": {
        "@expo/vector-icons": "^10.0.0",
        "@react-native-community/netinfo": "4.6.0",
        "@types/algoliasearch": "^3.34.7",
        "@types/react-instantsearch-native": "^5.3.3",
        "algoliasearch": "^3.35.1",
        "build-url": "^2.0.0",
        "cross-fetch": "^3.0.4",
        "expo": "^36.0.0",
        "expo-analytics": "^1.0.9",
        "expo-intent-launcher": "~8.0.0",
        "expo-location": "~8.0.0",
        "extend": "^3.0.2",
        "history": "^4.10.1",
        "intl": "^1.2.5",
        "jquery": "^3.0.0",
        "merge": "^1.2.1",
        "morgan": "^1.9.1",
        "native-base": "^2.13.8",
        "ramda": "^0.26.1",
        "react": "16.9.0",
        "react-instantsearch-native": "^6.1.0",
        "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.1.tar.gz",
        "react-native-dotenv": "^0.2.0",
        "react-native-gesture-handler": "~1.5.0",
        "react-native-map-link": "^2.1.4",
        "react-native-markdown-renderer": "^3.2.8",
        "react-native-modal": "^11.5.3",
        "react-native-reanimated": "~1.4.0",
        "react-native-tab-view": "^2.11.0",
        "react-redux": "^7.1.3",
        "react-router-native": "^5.1.2",
        "redux": "^4.0.5",
        "redux-persist": "^6.0.0",
        "redux-saga": "^1.1.3",
        "request": "^2.88.0",
        "sentry-expo": "~2.0.0",
        "ts-node": "^8.5.4",
        "uuid": "^3.3.2"
    },
    "lingui": {
        "sourceLocale": "en",
        "format": "po",
        "srcPathDirs": [
            "<rootDir>/lib"
        ]
    }
}
