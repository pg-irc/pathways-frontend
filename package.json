{
  "name": "pathways-frontend",
  "version": "1.68.0",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@lingui/babel-preset-react": "^2.9.2",
    "@lingui/cli": "^2.9.2",
    "@lingui/macro": "^2.9.2",
    "@lingui/react": "^2.9.2",
    "@types/history": "^4.7.8",
    "@types/jest": "^26.0.23",
    "@types/ramda": "0.26.39",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-native": "^5.1.0",
    "@types/uuid": "^3.4.6",
    "ajv": "^8.6.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-macros": "^2.8.0",
    "babel-preset-expo": "8.3.0",
    "jest-expo": "^41.0.0",
    "js-yaml": "^3.14.0",
    "react-dom": "16.13.1",
    "ts-jest": "^26.5.0",
    "tslint": "^6.1.3",
    "tslint-immutable": "^6.0.1",
    "typescript": "^4.3.2"
  },
  "main": "./node_modules/expo/AppEntry.js",
  "scripts": {
    "postinstall": "bin/postinstall.sh",
    "lint": "tslint src/**/*.ts src/**/*.tsx",
    "build": "bin/check_for_deploy.sh && bin/buildFixtures.sh && tsc",
    "test": "node ./node_modules/jest/bin/jest.js",
    "cbt": "yarn clean && yarn build && yarn test",
    "bt": "yarn build && yarn test",
    "clean": "rimraf 'lib/**/*.js' 'lib/**/*.js.map' 'locale/_build/**/*.json' 'tsconfig.tsbuildinfo'",
    "extract-strings": "lingui extract",
    "extract-strings-clean": "lingui extract --clean",
    "build-strings": "lingui compile --strict",
    "clean-strings": "rimraf 'locale/_build/**/*.json'",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/",
      "/helpers/"
    ],
    "setupFiles": [
      "./jestSetupFile.js"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.0",
    "@react-native-async-storage/async-storage": "^1.13.0",
    "@react-native-community/netinfo": "6.0.0",
    "@types/lodash.throttle": "^4.1.6",
    "build-url": "^6.0.1",
    "cross-fetch": "^3.1.4",
    "expo": "^41.0.0",
    "expo-analytics": "^1.0.16",
    "expo-intent-launcher": "~9.0.0",
    "expo-location": "~12.0.4",
    "expo-notifications": "~0.11.6",
    "expo-status-bar": "~1.0.4",
    "expo-updates": "~0.5.4",
    "history": "^4.10.1",
    "intl": "^1.2.5",
    "jest": "^26.6.3",
    "lodash.throttle": "^4.1.1",
    "native-base": "^2.15.2",
    "ramda": "^0.27.1",
    "react": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-41.0.0.tar.gz",
    "react-native-dotenv": "^0.2.0",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-keyboard-aware-scroll-view": "^0.9.3",
    "react-native-map-link": "^2.7.20",
    "react-native-markdown-display": "^7.0.0-alpha.2",
    "react-native-markdown-renderer": "^3.2.8",
    "react-native-modal": "^11.6.1",
    "react-native-reanimated": "~2.1.0",
    "react-native-swiper": "^1.6.0",
    "react-native-tab-view": "^2.15.2",
    "react-redux": "^7.2.1",
    "react-router-native": "^5.2.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "request": "^2.88.2",
    "sentry-expo": "^3.1.0",
    "ts-node": "^10.0.0",
    "uuid": "^3.4.0"
  },
  "lingui": {
    "sourceLocale": "en",
    "format": "po",
    "srcPathDirs": [
      "<rootDir>/lib"
    ]
  }
}
