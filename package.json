{
  "name": "pathways-frontend",
  "version": "1.44.0",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@lingui/babel-preset-react": "^2.9.1",
    "@lingui/cli": "^2.9.1",
    "@lingui/macro": "^2.9.1",
    "@lingui/react": "^2.9.1",
    "@types/history": "^4.7.6",
    "@types/jest": "^26.0.3",
    "@types/ramda": "0.26.39",
    "@types/react": "~16.9.41",
    "@types/react-native": "~0.62.13",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-native": "^5.1.0",
    "@types/uuid": "^3.4.6",
    "ajv": "^6.12.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-macros": "^2.8.0",
    "babel-preset-expo": "^8.2.3",
    "jest-expo": "^38.0.0",
    "js-yaml": "^3.14.0",
    "react-dom": "16.11.0",
    "ts-jest": "^26.1.0",
    "tslint": "^6.1.2",
    "tslint-immutable": "^6.0.1",
    "typescript": "~3.9.5"
  },
  "main": "./node_modules/expo/AppEntry.js",
  "scripts": {
    "postinstall": "bin/postinstall.sh",
    "lint": "tslint src/**/*.ts src/**/*.tsx",
    "build": "bin/check_for_deploy.sh && bin/buildFixtures.sh && tsc",
    "test": "node ./node_modules/jest/bin/jest.js",
    "cbt": "yarn clean && yarn build && yarn test",
    "bt": "yarn build && yarn test",
    "clean": "rimraf 'lib/**/*.js' 'lib/**/*.js.map' 'locale/_build/**/*.json'",
    "extract-strings": "lingui extract",
    "extract-strings-clean": "lingui extract --clean",
    "build-strings": "lingui compile --strict",
    "clean-strings": "rimraf 'locale/_build/**/*.json'",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/",
      "/helpers/"
    ],
    "setupFiles": [
      "./jestSetupFile.js"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^10.2.0",
    "@react-native-community/async-storage": "~1.11.0",
    "@react-native-community/netinfo": "5.9.2",
    "build-url": "^2.0.0",
    "cross-fetch": "^3.0.5",
    "expo": "^38.0.0",
    "expo-analytics": "^1.0.9",
    "expo-intent-launcher": "~8.2.1",
    "expo-location": "~8.2.1",
    "expo-notifications": "~0.3.3",
    "expo-status-bar": "^1.0.0",
    "history": "^4.10.1",
    "intl": "^1.2.5",
    "native-base": "^2.13.12",
    "ramda": "0.26.1",
    "react": "16.11.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-38.0.0.tar.gz",
    "react-native-dotenv": "^0.2.0",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-map-link": "^2.7.3",
    "react-native-markdown-display": "^6.1.5",
    "react-native-markdown-renderer": "^3.2.8",
    "react-native-modal": "^11.5.6",
    "react-native-reanimated": "~1.9.0",
    "react-native-swiper": "^1.6.0",
    "react-native-tab-view": "^2.14.4",
    "react-redux": "^7.2.0",
    "react-router-native": "^5.2.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "request": "^2.88.2",
    "sentry-expo": "^2.1.2",
    "ts-node": "^8.10.2",
    "uuid": "^3.4.0"
  },
  "lingui": {
    "sourceLocale": "en",
    "format": "po",
    "srcPathDirs": [
      "<rootDir>/lib"
    ]
  }
}