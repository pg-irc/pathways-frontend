{
    "name": "pathways-frontend",
    "version": "1.0.22",
    "private": true,
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@lingui/babel-preset-react": "2.7.3",
        "@lingui/cli": "2.7.3",
        "@lingui/react": "2.7.3",
        "@types/expo": "^32.0.0",
        "@types/history": "^4.7.0",
        "@types/jest": "^23.3.2",
        "@types/query-string": "^6.1.0",
        "@types/ramda": "^0.25.38",
        "@types/react": "^16.0.40",
        "@types/react-native": "^0.52.20",
        "@types/react-redux": "^5.0.21",
        "@types/react-router-native": "^4.2.3",
        "@types/react-test-renderer": "^16.0.1",
        "@types/uuid": "^3.4.4",
        "ajv": "^6.5.3",
        "babel-core": "^7.0.0-bridge.0",
        "babel-plugin-macros": "^2.4.2",
        "babel-preset-expo": "^5.0.0",
        "handlebars": "^4.1.2",
        "jest-expo": "^33.0.0",
        "js-yaml": "^3.13.1",
        "react-dom": "^16.5.0",
        "react-test-renderer": "^16.3.1",
        "ts-jest": "^23.10.5",
        "tslint": "^5.11.0",
        "tslint-immutable": "^4.7.0",
        "typescript": "^3.0.3"
    },
    "main": "./node_modules/expo/AppEntry.js",
    "scripts": {
        "postinstall": "bin/postinstall.sh",
        "lint": "tslint src/**/*.ts src/**/*.tsx",
        "build": "bin/buildFixtures.sh && tsc",
        "test": "node ./node_modules/jest/bin/jest.js",
        "cbt": "yarn clean && yarn build && yarn test",
        "bt": "yarn build && yarn test",
        "clean": "rimraf 'lib/**/*.js' 'lib/**/*.js.map' 'locale/_build/**/*.json'",
        "extract-strings": "lingui extract",
        "extract-strings-clean": "lingui extract --clean",
        "build-strings": "lingui compile --strict",
        "clean-strings": "rimraf 'locale/_build/**/*.json'",
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios"
    },
    "jest": {
        "preset": "jest-expo",
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testPathIgnorePatterns": [
            "/node_modules/",
            "/lib/",
            "/helpers/"
        ],
        "transformIgnorePatterns": [
			"node_modules/?!(react-native|native-base)"
		]
    },
    "dependencies": {
        "@expo/vector-icons": "^9.0.0",
        "expo": "^33.0.0",
        "expo-analytics": "^1.0.7",
        "expo-constants": "~5.0.0",
        "expo-location": "^4.0.0",
        "expo-react-native-adapter": "^3.0.1",
        "extend": "^3.0.2",
        "history": "^4.7.2",
        "intl": "^1.2.5",
        "jquery": "^3.0.0",
        "merge": "^1.2.1",
        "morgan": "^1.9.1",
        "native-base": "^2.11.0",
        "query-string": "^6.1.0",
        "ramda": "^0.25.0",
        "react": "16.8.3",
        "react-native": "https://github.com/expo/react-native/archive/sdk-33.0.0.tar.gz",
        "react-native-dotenv": "^0.2.0",
        "react-native-map-link": "^2.1.4",
        "react-native-markdown-renderer": "^3.2.8",
        "react-native-modal": "^7.0.2",
        "react-redux": "^5.0.7",
        "react-router-native": "^4.3.0",
        "redux": "^4.0.0",
        "redux-persist": "^5.9.1",
        "redux-saga": "^0.16.0",
        "request": "^2.88.0",
        "sentry-expo": "~1.13.0",
        "uuid": "^3.3.2"
    },
    "lingui": {
        "sourceLocale": "en",
        "format": "po",
        "srcPathDirs": [
            "<rootDir>/lib"
        ]
    }
}
